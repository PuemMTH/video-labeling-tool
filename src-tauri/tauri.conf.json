{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "video-labeling",
  "version": "0.1.0",
  "identifier": "com.puemmth.video-labeling",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Video Labeling Tool",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self' asset: https://asset.localhost file:; img-src 'self' asset: https://asset.localhost file: blob: data:; media-src * asset: https://asset.localhost file: blob: data: filesystem:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval';",
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$HOME/**",
          "**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}